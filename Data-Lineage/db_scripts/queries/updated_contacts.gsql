CREATE QUERY updated_contacts() FOR GRAPH MyGraph { 
/*
	Find all the Contacts that have been updated since creation.

	Starting with all Contact vertices,
	(1) Print only the contacts with Updated as true
*/

  	start = {Contact.*}; // start with all contacts
	
	updated = SELECT s FROM start:s
	          WHERE s.Updated == TRUE; // checks if attribute is true
	
	PRINT updated; // print result
}